{
	"name": "planning",
	"version": "0.1.0",
	"description": "",
	"main": "dist/index.js",
	"scripts": {
		"start": "cross-env NODE_ENV=production node ./bld/server/index.js",
		"debug": "node --inspect-brk=9229 ./bld/server/index.js",
		"build": "cross-env NODE_ENV=production webpack",
		"build:dev": "webpack",
		"dev": "concurrently --kill-others \"npm run dev:server\" \"npm run build:server:dev -- --watch\" \"nodemon --watch bld ./bld/server/index.js --port=8000\"",
		"dev:server": "ts-node --project ./tsconfig.tests.json ./src/entries/server/dev.ts",
		"build:server": "cross-env NODE_ENV=production webpack --config=./config/webpack.server.config",
		"build:server:dev": "webpack --config=./config/webpack.server.config",
		"storybook": "start-storybook -p 8080 -c .storybook",
		"preview": "start-storybook -p 8080 -c .storybook/preview",
		"test": "tape -r ./config/tests/require",
		"testc": "nyc --require ./config/tests/require tape",
		"testw": "nodemon -r ./config/tests/require node_modules/tape/bin/tape",
		"tests": "tape -r ./config/tests/require **/*.spec.ts",
		"testsc": "nyc --require ./config/tests/require tape **/*.spec.ts"
	},
	"dependencies": {
		"@types/react-redux": "^6.0.6",
		"@types/redux": "^3.6.0",
		"body-parser": "^1.18.3",
		"classnames": "^2.2.6",
		"cross-env": "^5.1.1",
		"encaps": "^0.3.3",
		"express": "^4.16.3",
		"minimist": "^1.2.0",
		"morgan": "^1.9.1",
		"node-static": "^0.7.9",
		"react": "^16.4.1",
		"react-cached-callback": "^1.0.1",
		"react-dom": "^16.4.1",
		"react-redux": "^5.0.7",
		"redux": "^4.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.0.0-beta.54",
		"@babel/preset-env": "^7.0.0-beta.54",
		"@storybook/react": "^4.0.0-alpha.14",
		"@types/enzyme": "^3.1.4",
		"@types/express": "^4.16.0",
		"@types/node": "^8.0.31",
		"@types/react": "^16.4.6",
		"@types/react-dom": "^16.0.6",
		"@types/sinon": "^4.0.0",
		"@types/tape": "^4.2.31",
		"babel-loader": "^8.0.0-beta.4",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-react": "^7.0.0-beta.3",
		"clean-webpack-plugin": "^0.1.19",
		"concurrently": "^3.6.0",
		"css-loader": "^1.0.0",
		"enzyme": "^3.3.0",
		"express-http-proxy": "^1.2.0",
		"file-loader": "^1.1.11",
		"less": "^3.8.1",
		"less-loader": "^4.0.5",
		"mini-css-extract-plugin": "^0.4.1",
		"mock-css-modules": "^1.0.0",
		"nodemon": "^1.18.3",
		"nyc": "^13.0.0",
		"react-hot-loader": "^4.3.3",
		"sinon": "^4.1.2",
		"style-loader": "^0.21.0",
		"tape": "^4.8.0",
		"tape-catch": "^1.0.6",
		"ts-loader": "^4.4.2",
		"ts-node": "^3.3.0",
		"tsconfig-paths": "^2.3.0",
		"typescript": "^3.0.3",
		"url-loader": "^1.0.1",
		"webpack": "^4.16.1",
		"webpack-cli": "^3.1.0",
		"webpack-dev-middleware": "^1.12.2",
		"webpack-dev-server": "^2.9.4",
		"webpack-hot-middleware": "^2.22.3",
		"webpack-node-externals": "^1.6.0"
	},
	"author": "Lebedev Mikhail <lebedev.mv@tutu.ru>",
	"license": "MIT",
	"nyc": {
		"include": [
			"src/**/*.tsx",
			"src/**/*.ts",
			"src/**/*.jsx",
			"src/**/*.js"
		],
		"exclude": [
			"src/**/*.spec.*"
		],
		"extension": [
			".jsx",
			".js",
			".tsx",
			".ts"
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/megazazik/planning.git"
	}
}
